        <RollingFile name="file" fileName="/tmp/eewd.log" filePattern="/tmp/eewd-%d{MM-dd-yyyy}-%i.log.gz">
            <PatternLayout pattern="%d %-5p [%t] %C{2} (%F:%L) - %m%n"/>
            <Policies>
                <TimeBasedTriggeringPolicy />
            </Policies>
        </RollingFile>
-->
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="console"/>
            <!--AppenderRef ref="file"/-->
        </Root>
    </Loggers>
</Configuration>